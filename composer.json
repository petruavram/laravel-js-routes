{
    "name": "petruavram/laravel-js-routes",
    "description": "Laravel routes from Javascript",
    "license": "MIT",
    "keywords": ["laravel", "generators", "artisan", "routing"],
    "authors": [
        {
            "name": "Fede Isas",
            "email": "fedeisas@hotmail.com"
        },
        {
            "name": "Petru Vasile Avram",
            "email": "pavram@gmab.ro"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.2",
        "illuminate/console": "~4.2",
        "illuminate/filesystem": "~4.2",
        "illuminate/routing": "~4.2"
    },
    "require-dev": {
        "illuminate/events": "~4.2",
        "phpunit/phpunit": "3.7.27",
        "mockery/mockery": "0.8.0",
        "squizlabs/php_codesniffer": "1.*",
        "jakub-onderka/php-parallel-lint": "*",
        "jakub-onderka/php-console-highlighter": "dev-master",
        "phpmd/phpmd": "1.5.*"
    },
    "autoload": {
        "psr-4": {
            "Fedeisas\\LaravelJsRoutes\\": "src/"
        },
        "classmap": [
            "src/Commands",
            "src/Generators"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
